#!/bin/sh

env_dir=$1
whitelist_regex=${2:-''}
blacklist_regex=${3:-'^(PATH|GIT_DIR|CPATH|CPPATH|LD_PRELOAD|LIBRARY_PATH)$'}
if [ -d "$env_dir" ]; then
  for e in $(ls $env_dir); do
    echo "$e";
  done
fi

printenv $PROJECT
printenv $PROJECT_GIT_URL

export project=$(printenv $PROJECT)
export github_url=$(printenv $PROJECT_GIT_URL)

git clone $github_url $project
mv $project "/app/"
